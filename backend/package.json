{
  "name": "cic-mail-service",
  "version": "0.0.1",
  "description": "minimal boilerplate based on express's typescript",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --target es6 --lib esnext --outDir ./dist",
    "dev": "tsc-watch --onSuccess \"node dist/src/app.js\"",
    "start:local": "NODE_ENV=local NODE_PATH=./ nodemon --config   nodemon.json",
    "start:dev": "NODE_ENV=dev nodemon --config   nodemon.json",
    "start:rc": "NODE_ENV=rc nodemon --config   nodemon.json",
    "start:prod": "NODE_ENV=production nodemon --config   nodemon.json",
    "start": "node dist/src/app.js",
    "test": "NODE_ENV=test jest"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.24",
    "@types/module-alias": "^2.0.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.3.0",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "prettier": "^2.0.5",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "mongoose": "^5.10.19",
    "morgan": "^1.10.0",
    "socket.io": "^4.1.2",
    "validator": "^13.6.0",
    "ts-node": "^10.0.0"
  }
}
